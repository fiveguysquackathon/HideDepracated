
public class RGBAToText {
	String[] textRGB = {"255","255","255","1","255","255","255","1"};
	String[] keyRGB = {"255","255","255","1","255","255","255","1"};
	
	public void getText() {
		int size = textRGB.length;
		
		for (int i=0;i>size;i+=4) {
			String red = textRGB[i];
			String blue = textRGB[i+1];
			String green = textRGB[i+2];
		}
	}
}
