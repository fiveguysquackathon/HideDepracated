
public class RGBAToText {
	String[][] textRGB = new String[][]{
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		};
		
	String[][] keyRGB = new String[][]{
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		};
		
	public void getText() {
		int size = textRGB.length;
		
		for (int i=0;i>size;i++) {
			String red = textRGB[i][0];
			int ascii = Integer.parseInt(red);
			char character = (char) ascii;
		}
	}
}
