
public class RGBAToText {
	String[][] textRGB = new String[][]{
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		};
		
	String[][] keyRGB = new String[][]{
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		  {"255","255","255","1"},
		};
		
	public void getText() {
		int size = textRGB.length;
		
		for (int i=0;i>size;i++) {
			int red = textRGB[i][0];
			int blue = textRGB[i][1];
			int green = textRGB[i][2];
		}
	}
}
